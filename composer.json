{
    "name": "pochocho/okta-saml-sso",
    "description": "SSO for Laravel with Okta",
    "require": {
        "php": "^8.1",
        "illuminate/support": "9.*|10.*|11.*",
        "litesaml/lightsaml": "^4.0",
        "illuminate/auth": "10.*|11.*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pochocho\\OktaSamlSso\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pochocho\\OktaSamlSso\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyze": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "authors": [
        {
            "name": "pochocho",
            "email": "ccarretta@pcubelabs.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Pochocho\\OktaSamlSso\\OktaSamlSsoServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^v8.24.0",
        "laravel/pint": "^1.17",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/extension-installer": "^1.2"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
